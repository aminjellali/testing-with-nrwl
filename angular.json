{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "widgets-project": {
      "root": "apps/widgets-project/",
      "sourceRoot": "apps/widgets-project/src",
      "projectType": "application",
      "prefix": "widgets-project",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/widgets-project",
            "index": "apps/widgets-project/src/index.html",
            "main": "apps/widgets-project/src/main.ts",
            "polyfills": "apps/widgets-project/src/polyfills.ts",
            "tsConfig": "apps/widgets-project/tsconfig.app.json",
            "assets": [
              "apps/widgets-project/src/favicon.ico",
              "apps/widgets-project/src/assets"
            ],
            "styles": ["apps/widgets-project/src/styles.css"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/widgets-project/src/environments/environment.ts",
                  "with": "apps/widgets-project/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "widgets-project:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "widgets-project:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "widgets-project:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/widgets-project/tsconfig.app.json",
              "apps/widgets-project/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/widgets-project/jest.config.js",
            "tsConfig": "apps/widgets-project/tsconfig.spec.json",
            "setupFile": "apps/widgets-project/src/test-setup.ts"
          }
        }
      }
    },
    "widgets-project-e2e": {
      "root": "apps/widgets-project-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
