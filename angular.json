            "cypressConfig": "apps/widgets-project-e2e/cypress.json",
            "tsConfig": "apps/widgets-project-e2e/tsconfig.e2e.json",
            "devServerTarget": "widgets-project:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "widgets-project:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/widgets-project-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
