            "tsConfig": [
              "libs/utils-service/tsconfig.lib.json",
              "libs/utils-service/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/utils-service/jest.config.js",
            "tsConfig": "libs/utils-service/tsconfig.spec.json",
            "setupFile": "libs/utils-service/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "comparator": {
      "root": "libs/comparator",
      "sourceRoot": "libs/comparator/src",
      "projectType": "library",
      "prefix": "widgets-project",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/comparator/tsconfig.lib.json",
              "libs/comparator/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/comparator/jest.config.js",
            "tsConfig": "libs/comparator/tsconfig.spec.json",
            "setupFile": "libs/comparator/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "hello": {
      "root": "apps/hello/",
      "sourceRoot": "apps/hello/src",
      "projectType": "application",
      "prefix": "widgets-project",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/hello",
            "index": "apps/hello/src/index.html",
            "main": "apps/hello/src/main.ts",
            "polyfills": "apps/hello/src/polyfills.ts",
            "tsConfig": "apps/hello/tsconfig.app.json",
            "assets": ["apps/hello/src/favicon.ico", "apps/hello/src/assets"],
            "styles": ["apps/hello/src/styles.css"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hello/src/environments/environment.ts",
                  "with": "apps/hello/src/environments/environment.prod.ts"
                }
              ],
